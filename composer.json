{
    "name": "yueyunying/laravel-dompdf",
    "description": "A DOMPDF Wrapper for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "dompdf", "pdf"],
    "authors": [
        {
            "name": "yueyunying",
            "email": "978840018@qq.com"
        }
    ],
    "require": {
        "php": ">=7",
        "illuminate/support": "^5.5|^6|^7|^8",
        "yueyunying/dompdf": "1.0.1"
    },

    "autoload": {
        "psr-4": {
            "Barryvdh\\DomPDF\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Barryvdh\\DomPDF\\ServiceProvider"
            ],
            "aliases": {
                "PDF": "Barryvdh\\DomPDF\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}
